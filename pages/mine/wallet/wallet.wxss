/* pages/mine/wallet/wallet.wxss */
/* 账户可提现余额 */
page{
  background: #f8f8f8;
}
.wallet-wrap{
  width: 100%;
  height: 100%;
  background: #f8f8f8;
}
.wallet-header{
  width: 100%;
  height: 220rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
}
.wallet-header-left .remain{
  margin-left:30rpx;
  font-size:28rpx;
  color:#333;
  margin-top:32rpx;

}
/* 钱 */
.wallet-header-right .wallet{
  font-size:40rpx;
  color:#e40012;
  margin-left:30rpx;
  margin-bottom:26px;
}

/* 提现等样式 */
.my-bottom{
  width: 100%;
  height: auto;
  margin-top: 20rpx;
  font-size: 26rpx;
  color: #333;
}
.li{
  width: 100%;
  height: 89rpx;
  background: #fff;
  border-bottom: 1px solid #F9F9F9;
  line-height: 90rpx;
  font-size: 26rpx;
  color:#333;
  display: flex;
  justify-content: space-between;
}

 .li text{
  margin-left: 26rpx; 
  font-size: 26rpx;
  color:#333;
  /* font-weight: bold; */
} 
.li .newDiv .news{
  width: 36rpx;
  height:36rpx;
  margin-left:30rpx;
  margin-bottom:-10rpx;
  

}
.jiantou{
  width: 12rpx;
  height: 24rpx;
  margin-top: 25rpx;
  margin-right: 40rpx;
}

.logout {
  width:80%;
  margin-top:12rpx;
  line-height:70rpx;
  text-align: center;
  background:#A5772C;
  color:white;
}
.my-bottom .li_friend{
  margin-top:20rpx;
}
.bottom-area {
  display: block;
  width: 40%;
  margin-left: auto;
  margin-right: auto;
  box-sizing: border-box; 
  font-size: 8rpx;
  text-align: center;
}

/* 新的钱包样式 */
.memberTop{
  width:690rpx;
  /* height:440rpx; */
  padding:30rpx 30rpx 0;
  background:#fff;
  border-bottom:20rpx solid #f9f9f9;

  /* position:absolute; */

}
.backPic{
  position:relative;
  margin-top:26rpx;

}
.backPic>image{
  width:690rpx;
  height:286rpx;
}
.actItem{
  width:590rpx;
  height:286rpx;
  position:absolute;
  top:0;
  left:0;
  padding:0 50rpx;
  font-family:PingFangSC-Medium;
}
.actItem .leftPull{
  padding-top:50rpx;
}
.actItem .rightPull{
  /* padding:40rpx 0; */
  padding:35rpx 0;
  margin-right: -25rpx;
}

.actItem .leftPull .cash{
  float:left;
  width:130rpx;
  height:42rpx;
  font-size:24rpx;
  border-radius:14rpx;
  text-align:center;
  line-height:42rpx;
  font-family:PingFangSC-Medium;
  margin-top: 40rpx;
}
.actItem .rightPull .cash,.actItem .rightPull .brokerage{
  float:right;
  width:130rpx;
  height:42rpx;
  font-size:24rpx;
  border-radius:14rpx;
  text-align:center;
  line-height:42rpx;
  font-family:PingFangSC-Medium;
}
.actItem .leftPull .cash{
  background:#f1e9df;
  color:#d24647;

}
.actItem .rightPull .brokerage{
 margin-right: 27rpx;
  color:#d24647;
  /* margin-top:28rpx; */
  background:#f1e9df;
  margin-top: 41rpx;
}
