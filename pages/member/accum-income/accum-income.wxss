/* pages/member/accum-income/accum-income.wxss */
page{
  background: #f9f9f9;
}
.container{
  width: 100%;
  height: auto;
}
.myearn-header{
  width: 100%;
  height: 210rpx;
  border-top:1px solid #F0F0F0;
  background: #fff;
  display: flex;
  justify-content: space-between;
}
.myearn-header-left{
  width: 50%;
  height: 210rpx;
  margin: 30rpx;
}
.myearn-header-right{
  width: 50%;
  height: 210rpx;
  margin: 30rpx;
}
.myearn-header-right text{
  display: inline-block;
  width: 120rpx;
  height: 44rpx;
  background:#CCA96A;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
  line-height: 40rpx;
  float: right;
}

.my-income-wrap{
  width: 100%;
  height: auto;
  background: #fff;
  border-bottom: 1px solid #ebebeb;
  border-top: 1px solid #ebebeb;
  display: flex;
  justify-content: space-between;
}
.income-list{
  width: 33%;
  height: auto;
  border-right: 1px solid #ebebeb;
  text-align: center;
  display: flex;
  flex-direction: column;
}

.distribute{
  width: 100%;
  height: 630rpx;
  margin-top: 20rpx;
  background: #fff;
}
.distribute-tit{
  width: 100%;
  padding: 50rpx 0;
  text-align: center;
  font-size: 30rpx;
  color:#333;
}
.bonus-income{
  width: 100%;
  height: auto;
  text-align: center;
  display: flex;
  flex-direction: column;
  font-size: 24rpx;
}

.distribute-wrap{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: space-between;
  margin-top: 24rpx;

}
.distribute-pic{
  width: 352rpx;
  height: 352rpx;
  border-radius: 50%;
  background: #CCA96A;
}
.income-table{
  width: 100%;
  height: auto;
}
.income-table-head{
  width: 100%;
  height: 70rpx;
  background: #CCA96A;
  display: flex;
  justify-content: space-between;
}
.income-table-head-list{
  width: 33%;
  height:70rpx;
  line-height: 70rpx;
  text-align: center;
  color:#fff;
  font-size: 26rpx; 
  border-right:1rpx solid #D9C5A1;
}